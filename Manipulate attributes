let student = {};

Object.defineProperty(student, 'name', {
  value: Gita,
  writable: true,
  enumerable:true,
  configurable:true
  });
  console.log(Object.getOwnPropertyDescriptor(student,'name');

//attributes can be manipulated only when the configurable attribute is set to be true

Object.defineProperty(student,'name', {
 value: Gitaa
 });
